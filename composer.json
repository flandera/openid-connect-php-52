{
    "name": "flandera/open_id_connect_client_52",
    "description": "PHP 5.2 library for connect to OpenID services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Flandera",
            "email": "flandera@volny.cz"
        }
    ],
    "require": {
        "php": ">=5.2",
        "phpseclib/phpseclib": "^1.0"
    },
    "include-path": ["open_id_connect_client_52/"],
    "autoload": {
        "psr-0": {
            "oidclient": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist",
        "secure-http": false,
        "disable-tls": true,
        "platform": {
            "php": "5.2.17"
        }
    }
}
